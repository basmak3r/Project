<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/CardStyle.css" rel="stylesheet" />
    <!-- CSS only -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
</head>
<body>
    @*<div class="form-style-10 m-2">
            <h1>Engineers Matrimony<span>Complete Your Profile!</span></h1>
        </div>*@
    <header>
        <!-- Navbar -->
        <nav class="navbar navbar-expand-lg fixed-top shadow-0">
            <div class="container">
                <a class="navbar-brand" href="#!"><span style="color: #FFD372;">Engineers</span><span style="color: #fff;">Matrimony</span></a>
                <button class="navbar-toggler" type="button" data-mdb-toggle="collapse"
                        data-mdb-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                        aria-label="Toggle navigation">
                    <i class="fas fa-bars"></i>
                </button>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav me-auto">
                        <li class="nav-item">
                            <div class="nav-link">
                                @Html.ActionLink("Home", "Index", "Basic_Profile")
                            </div>


                        </li>
                        <li class="nav-item">
                            <div class="nav-link">
                                @Html.ActionLink("Your Interests", "IntrestedProfilesView", "Basic_Profile")
                            </div>
                        </li>
                        <li class="nav-item">
                            <div class="nav-link">
                                @Html.ActionLink("Interested In You", "IncommingIntrest", "Basic_Profile")
                            </div>
                        </li>
                        <li class="nav-item">
                            <div class="nav-link">
                                @Html.ActionLink("Matches", "Mutual", "Basic_Profile")
                            </div>
                        </li>
                        <li class="nav-item">
                            <div class="nav-link">
                                
                                @Html.ActionLink("Logout","Logout", "Accounts")
                            </div>
                        </li>

                </div>
            </div>
        </nav>

    </header>
    <div>
        @RenderBody()
    </div>
</body>
</html>
